{
  "publisher": "causexl",
  "name": "atomic-css",
  "displayName": "Transform css module to atomic",
  "version": "0.0.9",
  "packageManager": "pnpm@7.8.0",
  "description": "",
  "author": "Cayden.Xiao",
  "license": "MIT",
  "homepage": "https://github.com/CauseXL/vscode-transform-scss-to-atomic",
  "repository": {
    "type": "git",
    "url": "https://github.com/CauseXL/vscode-transform-scss-to-atomic"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "assets/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.transform",
        "title": "test scss2"
      }
    ],
    "keybindings": [
      {
        "command": "extension.transform",
        "when": "editorHasSelection",
        "key": "ctrl+shift+K",
        "mac": "cmd+shift+K"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/ni": "^0.17.2",
    "@types/css-tree": "^2.3.1",
    "@types/node": "^18.6.4",
    "@types/node-sass": "^4.11.3",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "eslint-config-alloy": "^4.5.1",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "rimraf": "^3.0.2",
    "sass": "^1.59.3",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0",
    "css": "^3.0.0",
    "css-tree": "^2.3.1"
  }
}
