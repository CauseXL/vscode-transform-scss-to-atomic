{
  "publisher": "cause_xl",
  "name": "Transform css to atomic",
  "displayName": "transform scss/less to atomic class",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "",
  "author": "cause_xl <.com>",
  "license": "MIT",
  "homepage": "https://github.com/e#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.getCurrentFile",
        "title": "test scss"
      }
    ],
    "keybindings": [
      {
        "command": "extension.getCurrentFile",
        "when": "editorHasSelection",
        "key": "ctrl+shift+J",
        "mac": "cmd+shift+J"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@types/css-tree": "^2.3.1",
    "@types/node": "^18.6.4",
    "@types/node-sass": "^4.11.3",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "postcss-inherit": "^4.1.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.6",
    "rimraf": "^3.0.2",
    "sass": "^1.59.3",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0"
  },
  "dependencies": {
    "css": "^3.0.0",
    "css-tree": "^2.3.1",
    "gonzales-pe": "^4.3.0",
    "node-sass": "^8.0.0",
    "tailwindcss": "^3.2.7"
  }
}
